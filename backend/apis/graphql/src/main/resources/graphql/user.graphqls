type Query {
    users: [UserQueryResult]
}

type Mutation {
    registerUser(
        firstname: String!
        lastname: String!
        email: String!
        telephone: String!
    ): ID!
    renameUser(
        identifier: ID!
        firstname: String!
        lastname: String!
    ): Int!
}

type UserQueryResult {
    identifier: ID!
    firstname: String!
    lastname: String!
    email: String!
    telephone: String!
}
